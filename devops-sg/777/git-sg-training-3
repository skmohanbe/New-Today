}
stage('Maven Compile') {
sh label: 'Comile', script: 'mvn compile'
}
stage('Maven Test') {
sh label: 'Testing', script: 'mvn test'
}

stage('Maven Package') {
sh label: 'Testing', script: 'mvn package'
}

